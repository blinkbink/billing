<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping 
  SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="id.co.keriss.consolidate.ee.Bank" table="Bank">
    <id name="id" column="id" >
		<generator class="sequence">
				<param name="sequence">bank_sequence</param>
		</generator>
 	</id>
    <property name="name" type="string" length="20"></property>
    <property name="aquirer" type="boolean"></property>
    <property name="rekening" type="string" length="7"></property>
    <property name="bank_code" type="string" length="7"></property>
   
    </class>
    
</hibernate-mapping>